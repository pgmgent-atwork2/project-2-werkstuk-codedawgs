<section class="register">
  <h4>Add new user</h4>
  <a href="/login" class="register__back">&lt; Back</a>

  <form class="register-form" method="POST" action="/register">
    <input type="text" name="token" placeholder="Token" value="<%= input.value %>" required>
    <span class="register__error-msg"><%= input.err.msg %></span>
    <button class="register-form__button button" type="submit">Continue</button>
  </form>
  
</section>